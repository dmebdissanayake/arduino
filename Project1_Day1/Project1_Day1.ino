void setup() {
  // put your setup code here, to run once:
  pinMode (13,OUTPUT);
  pinMode (12,OUTPUT);
  pinMode (7,OUTPUT);
  pinMode (2,OUTPUT);
   

}

void loop() {
  // put your main code here, to run repeatedly:
  
 
  digitalWrite(13,LOW);
  
  digitalWrite(12,LOW);
  
  digitalWrite(7,LOW);
  
  digitalWrite(2,LOW);

  
   


}
